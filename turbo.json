{
  "$schema": "https://turbo.build/schema.json",
  "experimentalUI": true,
  "pipeline": {
    "build": {
      "env": ["NEXT_PUBLIC_AUTHOR", "GITHUB_TOKEN"],
      "outputs": [".next/**", "!.next/cache/**"]
